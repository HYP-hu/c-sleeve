@import "../../wxss/sleeve.wxss";

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 40rpx;
    background-color: #ffffff;
}

.inner-container {
    margin-top: 30rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.left {
    width: 346rpx;
    height: 522rpx;
    display: flex;
}

.right-container {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.title {
    width: 100%;
}

.right-size {
    width: 400rpx;
    height: 260rpx;
    display: flex;
}
