.sleeve{
    font-family: sleeve !important;
    color: #157658 !important;
}

.container{
    height: 100rpx;
    /*width: 100%;*/
    width: 750rpx;
    display: flex;
    flex-direction: row;
    background-color: #fff;
}

.btn-font{
    color: #fff;
    font-size: 32rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.buy{
    background-color: #157658;
    /*width: 260rpx;*/
    flex-grow: 1;
    /*flex:1;*/
    height: 100%;
}

.cart{
    background-color: #333;
    /*width: 260rpx;*/
    /*flex:1;*/
    flex-grow: 1;
    height: 100%;
}

.icons{
    box-sizing: border-box;
    padding-left: 16rpx;
    padding-right: 22rpx;
    display: flex;
    flex: row;
    align-items: center;
    justify-content: space-around;
    width: 230rpx;
    height: 100%;
    color: #999;
    flex-grow: 0;
}

.icons > view{
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.icons > view:nth-child(2){
    padding-top: 6rpx;
}

.icons text{
    font-size: 20rpx;
}